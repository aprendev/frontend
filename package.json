{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "format:raw": "prettier --write",
    "format": "yarn format:raw '**/*.{js,jsx,ts,tsx,json,vue,css,scss,less,graphql,gql,md,mdx,yml,yaml}'",
    "lint": "yarn lint:raw .",
    "lint:fix": "yarn lint:raw --fix .",
    "lint:raw": "eslint --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "axios": "^0.19.1",
    "bootstrap": "^4.4.1",
    "material-icons-react": "^1.0.4",
    "react": "^16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-typing-animation": "^1.6.2",
    "styled-components": "^5.0.0",
    "tabletop": "^1.6.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-lffg": "^10.0.1",
    "eslint-config-lffg-react": "^2.0.0",
    "prettier": "^1.19.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
